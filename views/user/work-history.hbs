<div class="container-fluid" style="background-color: #5c6263;height: 100vh;">
    <div class="row pt-5">
        <div class="col-md-12 mt-5">
            {{#if list}}
            <table class="table p-5 text-center text-light">
                <thead>
                    <tr>
                        <th scope="col">No</th>
                        <th scope="col">Workers Name</th>
                        <th scope="col">Worker Email</th>
                        <th scope="col">Worker Phone Number</th>
                        <th scope="col">Worker Type</th>
                        <th scope="col">Worker District</th>
                        <th scope="col">Worke Starting</th>
                        <th scope="col">Worke Ending</th>
                    </tr>
                </thead>
                <tbody>
                    {{#each list}}
                    {{#if this.workers}}
                        {{#if this.endstatus}}
                        <tr class="">
                            <td class="p-5">{{@index}}</td>
                            <td class="p-5">{{this.workers.name}}</td>
                            <td class="p-5">{{this.workers.email}}</td>
                            <td class="p-5">{{this.workers.ph}}</td>
                            <td class="p-5">{{this.workers.wrktype}}</td>
                            <td class="p-5">{{this.workers.district}}</td>
                            <td class="p-5">{{this.starting}}</td>
                            <td class="p-5">{{this.ending}}</td>
                        </tr>
                        {{/if}}
                    {{/if}}
                    {{/each}}
                </tbody>
            </table>
            {{else}}
                <h1 class="text-center text-light" style="font-weight: 800;">Your Not Choose Any Work Or Work In progress</h1>
            {{/if}}
        </div>
    </div>
</div>